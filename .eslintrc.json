{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "no-var": "off",
    "prefer-const": "warn",
    "no-redeclare": "off",
    "no-empty": "warn",
    "no-constant-condition": "warn",
    "no-undef": "error"
  },
  "globals": {
    "SQL": "readonly",
    "OZ": "readonly",
    "Dropbox": "readonly",
    "_": "readonly",
    "CONFIG": "readonly",
    "DATATYPES": "readonly",
    "LOCALE": "readonly",
    "ActiveXObject": "readonly"
  }
}
